default_values = {
  "hp": 6,
  "mp": 0,
  "sp": 6,
  "hd": "1d4",
  "md": "1d4",
  "sd": "1d4",
  "STR": 0,
  "DEX": 0,
  "CON": 0,
  "INT": 0,
  "WIS": 0,
  "CAR": 0
};


modifiers = {
    "red": {
        "hp": 4,
        "mp": 2,
        "hd_steps": 1,
        "md_steps": 1,
        "STR": 1,
        "COS": 1,
        "WIS": -1,
    },
    "orange": {
        "hp": 6,
        "hd_steps": 2,
        "STR": 1,
        "COS": 1,
        "INT": -1
    },
    "green": {
        "hp": 4,
        "mp": 2,
        "hd_steps": 1,
        "sd_steps": 1,
        "DEX": 1,
        "STR": 1,
        "INT": -1,
    },
