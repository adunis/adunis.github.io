output_list = []

for item in input_list:
    new_item = {}
    new_item["name"] = item["name"]
    new_item["crystals"] = {"requires": [], "provides": []}
    new_item["type"] = item["type"]
    new_item["moves"] = [{
            "moves_requirements": "cerulean",
            "moves_name": item["move"]["name"],
            "moves_description": item["move"]["description"]
        }]
    new_item["stats"] = [
            {"stat_name": "weight", "stat_value": item["weight"]},
            {"stat_name": "price", "stat_value": item["price"]}
        ]
    new_item["geo_location"] = {"latitude": 37.7749, "longitude": -122.4194}
    new_item["header_image"] = item["name"] + ".png"
    output_list.append(new_item)

# Convert the output list to a JSON string and print it
print(json.dumps(output_list, indent=2))